  "scripts": {
    "build-psql-csv-to-schema": "pwsh.exe .\\server\\psql-schema\\psql_table_creator\\generate_table.ps1 import-table -path server\\psql-schema\\",
    "build-psql-tables": "powershell.exe .\\scripts\\build-psql.ps1",
    "build-salesforce-schema": "powershell.exe .\\scripts\\build-salesforce.ps1",
    "build-psql": "npm run build-psql-csv-to-schema && npm run build-psql-tables && npm run build-salesforce-schema",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "node scripts/start-client.js",
    "build": "npm run tsc && node scripts/build.js",
    "start": "node server.js",
    "dev-start": "concurrently \"nodemy server.js\" \"node server.js\" \"npm run client\"",
    "heroku-postbuild": "npm install && npm run tsc && cd client && npm install --only=dev && npm install && npm run build",
    "tsc": "tsc --jsx react && cpx server/server.js ."
